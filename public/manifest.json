{
  "content_scripts": [
    {
      "js": ["content.js"],
      "css": ["content.css"],
      "matches": [
        "http://discord.com/*",
        "https://discord.com/*",
        "http://x.com/*",
        "https://x.com/*",
        "http://twitter.com/*",
        "https://twitter.com/*"
      ],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "manifest_version": 2,
  "name": "alfar-lm",
  "permissions": [
    "http://discord.com/*",
    "https://discord.com/*",
    "http://x.com/*",
    "https://x.com/*",
    "http://twitter.com/*",
    "https://twitter.com/*",
    "tabs",
    "cookies",
    "storage"
  ],
  "short_name": "login manager by @alfarc",
  "description": "author: https://t.me/+FozX3VZA0RIyNWY6",
  "version": "0.0.4",
  "browser_action": {
    "default_popup": "popup.html"
  }
}
